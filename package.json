  "license": "ISC",
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "node-sass": "^3.10.0",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "underscore": "^1.8.3"
  }
}
